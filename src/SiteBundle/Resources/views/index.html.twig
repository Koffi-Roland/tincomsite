{% block body %}

    <div id="wrapper">
        <!-- toggle top area -->
        <div class="hidden-top">
            <div class="hidden-top-inner container">
                <div class="row">
                    <div class="span12">
                        <MARQUEE behavior = "slide" direction="left"><strong>N'hésitez pas à nous contacter pour plus de renseignement &nbsp</strong>
