                                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                                <h4 id="myResetModalLabel">Reset your <strong>password</strong></h4>
                            </div>
                            <div class="modal-body">
                                <form class="form-horizontal">
                                    <div class="control-group">
                                        <label class="control-label" for="inputResetEmail">Email</label>
                                        <div class="controls">
                                            <input type="text" id="inputResetEmail" placeholder="Email">
                                        </div>
                                    </div>
                                    <div class="control-group">
                                        <div class="controls">
                                            <button type="submit" class="btn">Reset password</button>
                                        </div>
                                        <p class="aligncenter margintop20">
                                            We will send instructions on how to reset your password to your inbox
                                        </p>
                                    </div>
                                </form>
                            </div>
                        </div>
                        <!-- end reset modal -->
                    </div>
                </div>
                <div class="row">
                    <div class="span4">
                        <div class="logo">
                            <a href="{{path('accueil_sodigaz')}}"><img src="{{ asset('site/img/logo.png')}}" height="50px" width="150px" alt="" class="logo" /></a>
                        </div>
                    </div>
                    <div class="span8">
                        <div class="navbar navbar-static-top">
                            <div class="navigation">
                                <nav>
                                    {% import "menu_site.html" as html %}
                                    {% for parent in vglobal.getMenuArbre %}
                                        <ul class="nav topnav">
                                            <li class="dropdown">
                                                <a href="{{ parent.menu.lien }}">
                                                    <strong>{{ parent.menu.titre }}</strong>
                                                    {% if parent.arbre|length > 0 %}<i class="icon-angle-down"></i>{% endif %}
                                                </a>
                                                {% if parent.arbre|length > 0 %}
                                                    <ul class="dropdown-menu">
                                                        {{ html.afficherArbre(parent) }}
                                                    </ul>
                                                {% endif %}
                                            </li>
                                        </ul>
                                    {% endfor %}
                                </nav>
                            </div>
                            <!-- end navigation -->
                        </div>
                    </div>
                </div>
            </div>
        </header>
        <!-- end header -->

        {% block Panel %}

        {% endblock Panel %}


        {% block content %}

        {% endblock content %}

